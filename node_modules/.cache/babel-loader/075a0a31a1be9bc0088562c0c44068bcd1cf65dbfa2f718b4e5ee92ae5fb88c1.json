center', justifyContent: 'space-between', boxShadow: '0 2px 8px rgba(0, 0, 0, 0.1)' }\">\n    <div>\n      <a-row :gutter=\"16\" align=\"middle\">\n        <a-col>考生姓名</a-col>\n        <a-col>\n          <a-input v-model=\"inputName\" placeholder=\"姓名\" style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\" />\n        </a-col>\n        <a-col>考生分数</a-col>\n        <a-col>\n          <a-input v-model=\"inputScore\" placeholder=\"分数\" style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\" />\n        </a-col>\n        <a-col>\n          当前报考人: {{ currentCandidate }}\n        </a-col>\n      </a-row>\n    </div>\n    <div>\n      <a-space size=\"large\">\n        <a-button type=\"default\" @click=\"setCandidate\">设定报考人</a-button>\n        <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n      </a-space>\n    </div>\n  </a-layout-header>\n</template>\n\n<script>\nimport { mapState, mapMutations } from 'vuex';\n\nexport default {\n  name: 'AppHeader',\n  props: {\n    collapsed: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  data() {\n    return {\n      inputName: '',\n      inputScore: ''\n    };\n  },\n  computed: {\n    ...mapState(['name', 'score']),\n    currentCandidate() {\n      return `${this.name} - ${this.score}`;\n    }\n  },\n  methods: {\n    ...mapMutations(['setName', 'setScore', 'addArchive']),\n    setCandidate() {\n      this.setName(this.inputName);\n      this.setScore(this.inputScore);\n    },\n    createArchive() {\n      const archiveName = `${this.name} ${this.score}`;\n      let archivesList = this.$store.state.archivesList;\n\n      if (!archivesList.some(archive => archive.name === archiveName)) {\n        this.addArchive({ name: archiveName, data: [] });\n        alert(`${archiveName} 的报考档案构建成功`);\n      } else {\n        alert(`${archiveName} 的报考档案已存在`);\n      }\n    },\n  },\n  watch: {\n    name(newVal) {\n      this.inputName = newVal;\n    },\n    score(newVal) {\n      this.inputScore = newVal;\n    }\n  },\n  mounted() {\n    // Load the persisted name and score on mount\n    this.inputName = this.$store.state.name || '';\n    this.inputScore = this.$store.state.score || '';\n  }\n};\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>\n"],"mappings":"AA2BA,SAASA,QAAQ,EAAEC,YAAW,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGX,QAAQ,CAAC,CAAC,MAAM,EAAE,OAAO,CAAC,CAAC;IAC9BY,gBAAgBA,CAAA,EAAG;MACjB,OAAO,GAAG,IAAI,CAACV,IAAI,MAAM,IAAI,CAACW,KAAK,EAAE;IACvC;EACF,CAAC;EACDC,OAAO,EAAE;IACP,GAAGb,YAAY,CAAC,CAAC,SAAS,EAAE,UAAU,EAAE,YAAY,CAAC,CAAC;IACtDc,YAAYA,CAAA,EAAG;MACb,IAAI,CAACC,OAAO,CAAC,IAAI,CAACP,SAAS,CAAC;MAC5B,IAAI,CAACQ,QAAQ,CAAC,IAAI,CAACP,UAAU,CAAC;IAChC,CAAC;IACDQ,aAAaA,CAAA,EAAG;MACd,MAAMC,WAAU,GAAI,GAAG,IAAI,CAACjB,IAAI,IAAI,IAAI,CAACW,KAAK,EAAE;MAChD,IAAIO,YAAW,GAAI,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,YAAY;MAEjD,IAAI,CAACA,YAAY,CAACG,IAAI,CAACC,OAAM,IAAKA,OAAO,CAACtB,IAAG,KAAMiB,WAAW,CAAC,EAAE;QAC/D,IAAI,CAACM,UAAU,CAAC;UAAEvB,IAAI,EAAEiB,WAAW;UAAEX,IAAI,EAAE;QAAG,CAAC,CAAC;QAChDkB,KAAK,CAAC,GAAGP,WAAW,YAAY,CAAC;MACnC,OAAO;QACLO,KAAK,CAAC,GAAGP,WAAW,WAAW,CAAC;MAClC;IACF;EACF,CAAC;EACDQ,KAAK,EAAE;IACLzB,IAAIA,CAAC0B,MAAM,EAAE;MACX,IAAI,CAACnB,SAAQ,GAAImB,MAAM;IACzB,CAAC;IACDf,KAAKA,CAACe,MAAM,EAAE;MACZ,IAAI,CAAClB,UAAS,GAAIkB,MAAM;IAC1B;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACpB,SAAQ,GAAI,IAAI,CAACY,MAAM,CAACC,KAAK,CAACpB,IAAG,IAAK,EAAE;IAC7C,IAAI,CAACQ,UAAS,GAAI,IAAI,CAACW,MAAM,CAACC,KAAK,CAACT,KAAI,IAAK,EAAE;EACjD;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}