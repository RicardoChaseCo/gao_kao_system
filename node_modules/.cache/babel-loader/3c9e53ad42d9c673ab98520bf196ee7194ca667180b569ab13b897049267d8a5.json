{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  __name: 'AppHeader',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const name = computed({\n      get: () => store.state.studentInformation.name,\n      set: value => store.commit('setName', value)\n    });\n    const score = computed({\n      get: () => store.state.studentInformation.score,\n      set: value => store.commit('setScore', value)\n    });\n    const studentId = computed({\n      get: () => store.state.studentInformation.studentId,\n      set: value => store.commit('setStudentId', value)\n    });\n    const subject = computed({\n      get: () => store.state.studentInformation.subject,\n      set: value => store.commit('setSubject', value)\n    });\n    const setCandidate = () => {\n      alert('报考人信息输入成功');\n    };\n    const createArchive = () => {\n      const name = store.state.studentInformation.name;\n      if (!name) {\n        alert('请先输入考生信息');\n        return;\n      }\n      store.commit('addTableName', name);\n      alert('报考档案创建成功');\n    };\n    const __returned__ = {\n      store,\n      name,\n      score,\n      studentId,\n      subject,\n      setCandidate,\n      createArchive,\n      computed,\n      get useStore() {\n        return useStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","useStore","store","name","get","state","studentInformation","set","value","commit","score","studentId","subject","setCandidate","alert","createArchive"],"sources":["/Users/jiangxingnandecomputer/Desktop/sss/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header class=\"app-header\"\n    :style=\"{ marginLeft: collapsed ? '80px' : '210px', transition: 'margin-left 0.2s', background: '#f5f5f5', padding: '10px 24px', display: 'flex', alignItems: 'center', justifyContent: 'space-between' }\">\n    <div>\n      <a-row :gutter=\"16\" align=\"middle\">\n        <a-col>考生姓名</a-col>\n        <a-col>\n          <input v-model=\"name\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col>考生分数</a-col>\n        <a-col>\n          <input v-model=\"score\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col>考生考号</a-col>\n        <a-col>\n          <input v-model=\"studentId\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col>考生科目</a-col>\n        <a-col>\n          <input v-model=\"subject\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col class=\"styled-col compact-col\">\n          当前报考人: {{ name }} - {{ score }} - {{ studentId }} - {{ subject }}\n        </a-col>\n      </a-row>\n    </div>\n    <div>\n      <a-space size=\"large\">\n        <a-button type=\"default\" @click=\"setCandidate\">设定报考人</a-button>\n        <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n      </a-space>\n    </div>\n  </a-layout-header>\n</template>\n\n<script setup>\nimport { computed } from 'vue';\nimport { useStore } from 'vuex';\n\nconst store = useStore();\n\nconst name = computed({\n  get: () => store.state.studentInformation.name,\n  set: value => store.commit('setName', value)\n});\nconst score = computed({\n  get: () => store.state.studentInformation.score,\n  set: value => store.commit('setScore', value)\n});\nconst studentId = computed({\n  get: () => store.state.studentInformation.studentId,\n  set: value => store.commit('setStudentId', value)\n});\nconst subject = computed({\n  get: () => store.state.studentInformation.subject,\n  set: value => store.commit('setSubject', value)\n});\n\nconst setCandidate = () => {\n  alert('报考人信息输入成功');\n};\n\nconst createArchive = () => {\n  const name = store.state.studentInformation.name;\n\n  if (!name) {\n    alert('请先输入考生信息');\