{"ast":null,"code":"import { ref, computed, onMounted } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: 'HistoryView',\n  setup() {\n    const filters = ref({\n      subjectType: '',\n      batch: ''\n    });\n    const data = ref([]);\n    const filteredData = computed(() => {\n      if (!filters.value.subjectType || !filters.value.batch) {\n        return [];\n      }\n      return data.value.filter(item => item.subjectType === filters.value.subjectType && item.batch === filters.value.batch);\n    });\n    const columns = [{\n      title: '省份',\n      dataIndex: 'province',\n      key: 'province'\n    }, {\n      title: '学校类型',\n      dataIndex: 'schoolType',\n      key: 'schoolType'\n    }, {\n      title: '中外合作',\n      dataIndex: 'cooperation',\n      key: 'cooperation'\n    }, {\n      title: '学校',\n      dataIndex: 'school',\n      key: 'school'\n    }, {\n      title: '专业',\n      dataIndex: 'major',\n      key: 'major'\n    }, {\n      title: '分数',\n      dataIndex: 'score',\n      key: 'score'\n    }];\n    const handleFilter = () => {\n      // 过滤逻辑已经在 computed 属性中实现\n    };\n    onMounted(async () => {\n      const response = await axios.get('/history_data.json');\n      data.value = response.data;\n    });\n    return {\n      filters,\n      columns,\n      filteredData,\n      handleFilter\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","axios","name","setup","filters","subjectType","batch","data","filteredData","value","filter","item","columns","title","dataIndex","key","handleFilter","response","get"],"sources":["/Users/jiangxingnandecomputer/Desktop/gao_kao_system/src/views/HistoryView.vue"],"sourcesContent":["<template>\n  <div>\n    <a-form layout=\"inline\" @submit.prevent=\"handleFilter\">\n      <a-form-item label=\"再选\">\n        <a-select v-model=\"filters.subjectType\" placeholder=\"选择再选\" style=\"width: 200px\">\n          <a-select-option value=\"历史类\">历史类</a-select-option>\n          <a-select-option value=\"物理类\">物理类</a-select-option>\n        </a-select>\n      </a-form-item>\n      <a-form-item label=\"批次\">\n        <a-select v-model=\"filters.batch\" placeholder=\"选择批次\" style=\"width: 200px\">\n          <a-select-option value=\"提前批本科\">提前批本科</a-select-option>\n          <a-select-option value=\"普通批本科\">普通批本科</a-select-option>\n          <a-select-option value=\"提前批专科\">提前批专科</a-select-option>\n          <a-select-option value=\"普通批专科\">普通批专科</a-select-option>\n        </a-select>\n      </a-form-item>\n      <a-form-item>\n        <a-button type=\"primary\" @click=\"handleFilter\">筛选</a-button>\n      </a-form-item>\n    </a-form>\n\n    <div v-if=\"!filters.subjectType || !filters.batch\" style=\"margin-top: 20px;\">\n      <p>请设置再选和批次</p>\n    </div>\n    \n    <a-table\n      v-else\n      :columns=\"columns\"\n      :dataSource=\"filteredData\"\n      rowKey=\"school\"\n      style=\"margin-top: 20px;\"\n    ></a-table>\n  </div>\n</template>\n\n<script>\nimport { ref, computed, onMounted } from 'vue';\nimport axios from 'axios';\n\nexport default {\n  name: 'HistoryView',\n  setup() {\n    const filters = ref({\n      subjectType: '',\n      batch: '',\n    });\n\n    const data = ref([]);\n    const filteredData = computed(() => {\n      if (!filters.value.subjectType || !filters.value.batch) {\n        return [];\n      }\n      return data.value.filter(item => \n        item.subjectType === filters.value.subjectType &&\n        item.batch === filters.value.batch\n      );\n    });\n\n    const columns = [\n      {\n        title: '省份',\n        dataIndex: 'province',\n        key: 'province',\n      },\n      {\n        title: '学校类型',\n        dataIndex: 'schoolType',\n        key: 'schoolType',\n      },\n      {\n        title: '中外合作',\n        dataIndex: 'cooperation',\n        key: 'cooperation',\n      },\n      {\n        title: '学校',\n        dataIndex: 'school',\n        key: 'school',\n      },\n      {\n        title: '专业',\n        dataIndex: 'major',\n        key: 'major',\n      },\n      {\n        title: '分数',\n        dataIndex: 'score',\n        key: 'score',\n      },\n    ];\n\n    const handleFilter = () => {\n      // 过滤逻辑已经在 computed 属性中实现\n    };\n\n    onMounted(async () => {\n      const response = await axios.get('/history_data.json');\n      data.value = response.data;\n    });\n\n    return {\n      filters,\n      columns,\n      filteredData,\n      handleFilter,\n    };\n  },\n};\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>\n"],"mappings":"AAqCA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AAC9C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAKA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAIN,GAAG,CAAC;MAClBO,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE;IACT,CAAC,CAAC;IAEF,MAAMC,IAAG,GAAIT,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMU,YAAW,GAAIT,QAAQ,CAAC,MAAM;MAClC,IAAI,CAACK,OAAO,CAACK,KAAK,CAACJ,WAAU,IAAK,CAACD,OAAO,CAACK,KAAK,CAACH,KAAK,EAAE;QACtD,OAAO,EAAE;MACX;MACA,OAAOC,IAAI,CAACE,KAAK,CAACC,MAAM,CAACC,IAAG,IAC1BA,IAAI,CAACN,WAAU,KAAMD,OAAO,CAACK,KAAK,CAACJ,WAAU,IAC7CM,IAAI,CAACL,KAAI,KAAMF,OAAO,CAACK,KAAK,CAACH,KAC/B,CAAC;IACH,CAAC,CAAC;IAEF,MAAMM,OAAM,GAAI,CACd;MACEC,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE,UAAU;MACrBC,GAAG,EAAE;IACP,CAAC,EACD;MACEF,KAAK,EAAE,MAAM;MACbC,SAAS,EAAE,YAAY;MACvBC,GAAG,EAAE;IACP,CAAC,EACD;MACEF,KAAK,EAAE,MAAM;MACbC,SAAS,EAAE,aAAa;MACxBC,GAAG,EAAE;IACP,CAAC,EACD;MACEF,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE,QAAQ;MACnBC,GAAG,EAAE;IACP,CAAC,EACD;MACEF,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE,OAAO;MAClBC,GAAG,EAAE;IACP,CAAC,EACD;MACEF,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE,OAAO;MAClBC,GAAG,EAAE;IACP,CAAC,CACF;IAED,MAAMC,YAAW,GAAIA,CAAA,KAAM;MACzB;IAAA,CACD;IAEDhB,SAAS,CAAC,YAAY;MACpB,MAAMiB,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAAC,oBAAoB,CAAC;MACtDX,IAAI,CAACE,KAAI,GAAIQ,QAAQ,CAACV,IAAI;IAC5B,CAAC,CAAC;IAEF,OAAO;MACLH,OAAO;MACPQ,OAAO;MACPJ,YAAY;MACZQ;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}