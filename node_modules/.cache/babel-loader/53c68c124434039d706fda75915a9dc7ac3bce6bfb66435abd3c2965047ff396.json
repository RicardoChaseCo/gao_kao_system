{"ast":null,"code":"import { mapState, mapMutations, mapActions } from 'vuex';\nimport { useToast } from 'vue3-toastify';\nimport 'vue3-toastify/dist/index.css';\nexport default {\n  name: 'AppHeader',\n  props: {\n    collapsed: {\n      type: Boolean,\n      default: false\n    }\n  },\n  computed: {\n    ...mapState(['name', 'score'])\n  },\n  methods: {\n    ...mapMutations(['setName', 'setScore']),\n    ...mapActions(['createArchive']),\n    handleCreateArchive() {\n      if (this.name && this.score) {\n        this.createArchive();\n        this.$toast.success(`档案创建成功：${this.name} - ${this.score}`, {\n          position: 'top-right',\n          duration: 3000\n        });\n      } else {\n        this.$toast.error('请输入考生姓名和分数', {\n          position: 'top-right',\n          duration: 3000\n        });\n      }\n    }\n  },\n  watch: {\n    name(val) {\n      this.setName(val);\n      localStorage.setItem('name', val);\n    },\n    score(val) {\n      this.setScore(val);\n      localStorage.setItem('score', val);\n    }\n  },\n  mounted() {\n    const storedName = localStorage.getItem('name');\n    const storedScore = localStorage.getItem('score');\n    if (storedName) {\n      this.setName(storedName);\n    }\n    if (storedScore) {\n      this.setScore(storedScore);\n    }\n  },\n  setup() {\n    const $toast = useToast();\n    return {\n      $toast\n    };\n  }\n};","map":{"version":3,"names":["mapState","mapMutations","mapActions","useToast","name","props","collapsed","type","Boolean","default","computed","methods","handleCreateArchive","score","createArchive","$toast","success","position","duration","error","watch","val","setName","localStorage","setItem","setScore","mounted","storedName","getItem","storedScore","setup"],"sources":["/Users/jiangxingnandecomputer/Desktop/sss/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header\n    :style=\"{\n      marginLeft: collapsed ? '80px' : '110px',\n      transition: 'margin-left 0.2s',\n      background: '#fff',\n      padding: '0 24px',\n      display: 'flex',\n      alignItems: 'center',\n      justifyContent: 'space-between',\n      boxShadow: '0 2px 8px rgba(0, 0, 0, 0.1)'\n    }\"\n  >\n    <div>\n      <a-row :gutter=\"16\" align=\"middle\">\n        <a-col>考生姓名</a-col>\n        <a-col>\n          <a-input\n            v-model=\"name\"\n            placeholder=\"姓名\"\n            style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\"\n          />\n        </a-col>\n        <a-col>考生分数</a-col>\n        <a-col>\n          <a-input\n            v-model=\"score\"\n            placeholder=\"分数\"\n            style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\"\n          />\n        </a-col>\n      </a-row>\n      <div style=\"margin-top: 10px;\">\n        <span>当前考生：{{ name }} - {{ score }}</span>\n      </div>\n    </div>\n    <div>\n      <a-space size=\"large\">\n        <a-button type=\"primary\" @click=\"handleCreateArchive\">创建报考档案</a-button>\n      </a-space>\n    </div>\n  </a-layout-header>\n</template>\n\n<script>\nimport { mapState, mapMutations, mapActions } from 'vuex';\nimport { useToast } from 'vue3-toastify';\nimport 'vue3-toastify/dist/index.css';\n\nexport default {\n  name: 'AppHeader',\n  props: {\n    collapsed: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  computed: {\n    ...mapState(['name', 'score']),\n  },\n  methods: {\n    ...mapMutations(['setName', 'setScore']),\n    ...mapActions(['createArchive']),\n    handleCreateArchive() {\n      if (this.name && this.score) {\n        this.createArchive();\n        this.$toast.success(`档案创建成功：${this.name} - ${this.score}`, {\n          position: 'top-right',\n          duration: 3000,\n        });\n      } else {\n        this.$toast.error('请输入考生姓名和分数', {\n          position: 'top-right',\n          duration: 3000,\n        });\n      }\n    },\n  },\n  watch: {\n    name(val) {\n      this.setName(val);\n      localStorage.setItem('name', val);\n    },\n    score(val) {\n      this.setScore(val);\n      localStorage.setItem('score', val);\n    },\n  },\n  mounted() {\n    const storedName = localStorage.getItem('name');\n    const storedScore = localStorage.getItem('score');\n    if (storedName) {\n      this.setName(storedName);\n    }\n    if (storedScore) {\n      this.setScore(storedScore);\n    }\n  },\n  setup() {\n    const $toast = useToast();\n    return { $toast };\n  },\n};\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>\n"],"mappings":"AA6CA,SAASA,QAAQ,EAAEC,YAAY,EAAEC,UAAS,QAAS,MAAM;AACzD,SAASC,QAAO,QAAS,eAAe;AACxC,OAAO,8BAA8B;AAErC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGV,QAAQ,CAAC,CAAC,MAAM,EAAE,OAAO,CAAC;EAC/B,CAAC;EACDW,OAAO,EAAE;IACP,GAAGV,YAAY,CAAC,CAAC,SAAS,EAAE,UAAU,CAAC,CAAC;IACxC,GAAGC,UAAU,CAAC,CAAC,eAAe,CAAC,CAAC;IAChCU,mBAAmBA,CAAA,EAAG;MACpB,IAAI,IAAI,CAACR,IAAG,IAAK,IAAI,CAACS,KAAK,EAAE;QAC3B,IAAI,CAACC,aAAa,CAAC,CAAC;QACpB,IAAI,CAACC,MAAM,CAACC,OAAO,CAAC,UAAU,IAAI,CAACZ,IAAI,MAAM,IAAI,CAACS,KAAK,EAAE,EAAE;UACzDI,QAAQ,EAAE,WAAW;UACrBC,QAAQ,EAAE;QACZ,CAAC,CAAC;MACJ,OAAO;QACL,IAAI,CAACH,MAAM,CAACI,KAAK,CAAC,YAAY,EAAE;UAC9BF,QAAQ,EAAE,WAAW;UACrBC,QAAQ,EAAE;QACZ,CAAC,CAAC;MACJ;IACF;EACF,CAAC;EACDE,KAAK,EAAE;IACLhB,IAAIA,CAACiB,GAAG,EAAE;MACR,IAAI,CAACC,OAAO,CAACD,GAAG,CAAC;MACjBE,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEH,GAAG,CAAC;IACnC,CAAC;IACDR,KAAKA,CAACQ,GAAG,EAAE;MACT,IAAI,CAACI,QAAQ,CAACJ,GAAG,CAAC;MAClBE,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,GAAG,CAAC;IACpC;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,MAAMC,UAAS,GAAIJ,YAAY,CAACK,OAAO,CAAC,MAAM,CAAC;IAC/C,MAAMC,WAAU,GAAIN,YAAY,CAACK,OAAO,CAAC,OAAO,CAAC;IACjD,IAAID,UAAU,EAAE;MACd,IAAI,CAACL,OAAO,CAACK,UAAU,CAAC;IAC1B;IACA,IAAIE,WAAW,EAAE;MACf,IAAI,CAACJ,QAAQ,CAACI,WAAW,CAAC;IAC5B;EACF,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMf,MAAK,GAAIZ,QAAQ,CAAC,CAAC;IACzB,OAAO;MAAEY;IAAO,CAAC;EACnB;AACF,CAAC","ignoreList":[]},"me