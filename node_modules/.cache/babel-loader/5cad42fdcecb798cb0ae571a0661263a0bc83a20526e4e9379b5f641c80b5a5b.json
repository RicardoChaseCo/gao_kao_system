{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'AppHeader',\n  props: {\n    collapsed: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      name: localStorage.getItem('currentName') || '',\n      score: localStorage.getItem('currentScore') || '',\n      currentName: localStorage.getItem('currentName'),\n      currentScore: localStorage.getItem('currentScore')\n    };\n  },\n  methods: {\n    setCandidate() {\n      localStorage.setItem('currentName', this.name);\n      localStorage.setItem('currentScore', this.score);\n      this.currentName = this.name;\n      this.currentScore = this.score;\n    },\n    createArchive() {\n      const currentName = localStorage.getItem('currentName');\n      const currentScore = localStorage.getItem('currentScore');\n      const archiveName = `${currentName}${currentScore}`;\n      let archivesList = JSON.parse(localStorage.getItem('archivesList')) || [];\n      if (!archivesList.some(archive => archive.name === archiveName)) {\n        archivesList.push({\n          name: archiveName,\n          data: []\n        });\n        localStorage.setItem('archivesList', JSON.stringify(archivesList));\n        alert(`${archiveName} 的报考档案构建成功`);\n      } else {\n        alert(`${archiveName} 的报考档案已存在`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","props","collapsed","type","Boolean","default","data","localStorage","getItem","score","currentName","currentScore","methods","setCandidate","setItem","createArchive","archiveName","archivesList","JSON","parse","some","archive","push","stringify","alert"],"sources":["/Users/jiangxingnandecomputer/Desktop/sss/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header :style=\"{ marginLeft: collapsed ? '80px' : '110px', transition: 'margin-left 0.2s', background: '#fff', padding: '0 24px', display: 'flex', alignItems: 'center', justifyContent: 'space-between', boxShadow: '0 2px 8px rgba(0, 0, 0, 0.1)' }\">\n    <div>\n      <a-row :gutter=\"16\" align=\"middle\">\n        <a-col>考生姓名</a-col>\n        <a-col>\n          <a-input v-model=\"name\" placeholder=\"姓名\" style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\" />\n        </a-col>\n        <a-col>考生分数</a-col>\n        <a-col>\n          <a-input v-model=\"score\" placeholder=\"分数\" style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\" />\n        </a-col>\n        <a-col>\n          当前报考人: {{ currentName }} - {{ currentScore }}\n        </a-col>\n      </a-row>\n    </div>\n    <div>\n      <a-space size=\"large\">\n        <a-button type=\"default\" @click=\"setCandidate\">设定报考人</a-button>\n        <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n      </a-space>\n    </div>\n    <div v-if=\"currentName && currentScore\">\n      当前报考人: {{ currentName }} - {{ currentScore }}\n    </div>\n  </a-layout-header>\n</template>\n\n<script>\nexport default {\n  name: 'AppHeader',\n  props: {\n    collapsed: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  data() {\n    return {\n      name: localStorage.getItem('currentName') || '',\n      score: localStorage.getItem('currentScore') || '',\n      currentName: localStorage.getItem('currentName'),\n      currentScore: localStorage.getItem('currentScore')\n    };\n  },\n  methods: {\n    setCandidate() {\n      localStorage.setItem('currentName', this.name);\n      localStorage.setItem('currentScore', this.score);\n      this.currentName = this.name;\n      this.currentScore = this.score;\n    },\n    createArchive() {\n      const currentName = localStorage.getItem('currentName');\n      const currentScore = localStorage.getItem('currentScore');\n      const archiveName = `${currentName}${currentScore}`;\n      let archivesList = JSON.parse(localStorage.getItem('archivesList')) || [];\n\n      if (!archivesList.some(archive => archive.name === archiveName)) {\n        archivesList.push({ name: archiveName, data: [] });\n        localStorage.setItem('archivesList', JSON.stringify(archivesList));\n        alert(`${archiveName} 的报考档案构建成功`);\n      } else {\n        alert(`${archiveName} 的报考档案已存在`);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>"],"mappings":";AA8BA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLN,IAAI,EAAEO,YAAY,CAACC,OAAO,CAAC,aAAa,KAAK,EAAE;MAC/CC,KAAK,EAAEF,YAAY,CAACC,OAAO,CAAC,cAAc,KAAK,EAAE;MACjDE,WAAW,EAAEH,YAAY,CAACC,OAAO,CAAC,aAAa,CAAC;MAChDG,YAAY,EAAEJ,YAAY,CAACC,OAAO,CAAC,cAAc;IACnD,CAAC;EACH,CAAC;EACDI,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACbN,YAAY,CAACO,OAAO,CAAC,aAAa,EAAE,IAAI,CAACd,IAAI,CAAC;MAC9CO,YAAY,CAACO,OAAO,CAAC,cAAc,EAAE,IAAI,CAACL,KAAK,CAAC;MAChD,IAAI,CAACC,WAAU,GAAI,IAAI,CAACV,IAAI;MAC5B,IAAI,CAACW,YAAW,GAAI,IAAI,CAACF,KAAK;IAChC,CAAC;IACDM,aAAaA,CAAA,EAAG;MACd,MAAML,WAAU,GAAIH,YAAY,CAACC,OAAO,CAAC,aAAa,CAAC;MACvD,MAAMG,YAAW,GAAIJ,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC;MACzD,MAAMQ,WAAU,GAAI,GAAGN,WAAW,GAAGC,YAAY,EAAE;MACnD,IAAIM,YAAW,GAAIC,IAAI,CAACC,KAAK,CAACZ,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC,KAAK,EAAE;MAEzE,IAAI,CAACS,YAAY,CAACG,IAAI,CAACC,OAAM,IAAKA,OAAO,CAACrB,IAAG,KAAMgB,WAAW,CAAC,EAAE;QAC/DC,YAAY,CAACK,IAAI,CAAC;UAAEtB,IAAI,EAAEgB,WAAW;UAAEV,IAAI,EAAE;QAAG,CAAC,CAAC;QAClDC,YAAY,CAACO,OAAO,CAAC,cAAc,EAAEI,IAAI,CAACK,SAAS,CAACN,YAAY,CAAC,CAAC;QAClEO,KAAK,CAAC,GAAGR,WAAW,YAAY,CAAC;MACnC,OAAO;QACLQ,KAAK,CAAC,GAAGR,WAAW,WAAW,CAAC;MAClC;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}