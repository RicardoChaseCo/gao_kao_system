{"ast":null,"code":"import { ref, computed } from 'vue';\nimport DataTable from '../components/DataTable.vue';\nimport data from '../data/Book1.json'; // 导入本地 JSON 数据\n\nexport default {\n  name: 'HistoryView',\n  components: {\n    DataTable\n  },\n  setup() {\n    const filters = ref({\n      subjectType: '',\n      batch: ''\n    });\n    const isValidSelection = ref(false);\n    const filteredData = computed(() => {\n      if (!isValidSelection.value) {\n        return [];\n      }\n      return data.filter(item => item['再选'] === filters.value.subjectType && item['批次'] === filters.value.batch);\n    });\n    const handleFilter = () => {\n      isValidSelection.value = filters.value.subjectType === '物理类' && filters.value.batch === '普通批本科';\n    };\n    return {\n      filters,\n      isValidSelection,\n      filteredData,\n      handleFilter\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","DataTable","data","name","components","setup","filters","subjectType","batch","isValidSelection","filteredData","value","filter","item","handleFilter"],"sources":["/Users/jiangxingnandecomputer/Desktop/gao_kao_system/src/views/HistoryView.vue"],"sourcesContent":["<template>\n  <div>\n    <a-form layout=\"inline\" @submit.prevent=\"handleFilter\">\n      <a-form-item label=\"再选\">\n        <a-select v-model=\"filters.subjectType\" placeholder=\"选择再选\" style=\"width: 200px\">\n          <a-select-option value=\"历史类\">历史类</a-select-option>\n          <a-select-option value=\"物理类\">物理类</a-select-option>\n        </a-select>\n      </a-form-item>\n      <a-form-item label=\"批次\">\n        <a-select v-model=\"filters.batch\" placeholder=\"选择批次\" style=\"width: 200px\">\n          <a-select-option value=\"提前批本科\">提前批本科</a-select-option>\n          <a-select-option value=\"普通批本科\">普通批本科</a-select-option>\n          <a-select-option value=\"提前批专科\">提前批专科</a-select-option>\n          <a-select-option value=\"普通批专科\">普通批专科</a-select-option>\n        </a-select>\n      </a-form-item>\n      <a-form-item>\n        <a-button type=\"primary\" @click=\"handleFilter\">筛选</a-button>\n      </a-form-item>\n    </a-form>\n\n    <div v-if=\"!isValidSelection\" style=\"margin-top: 20px;\">\n      <p>请设置再选和批次</p>\n    </div>\n    \n    <DataTable v-else :data=\"filteredData\" />\n  </div>\n</template>\n\n<script>\nimport { ref, computed } from 'vue';\nimport DataTable from '../components/DataTable.vue';\nimport data from '../data/Book1.json'; // 导入本地 JSON 数据\n\nexport default {\n  name: 'HistoryView',\n  components: {\n    DataTable,\n  },\n  setup() {\n    const filters = ref({\n      subjectType: '',\n      batch: '',\n    });\n\n    const isValidSelection = ref(false);\n\n    const filteredData = computed(() => {\n      if (!isValidSelection.value) {\n        return [];\n      }\n      return data.filter(item => \n        item['再选'] === filters.value.subjectType &&\n        item['批次'] === filters.value.batch\n      );\n    });\n\n    const handleFilter = () => {\n      isValidSelection.value = filters.value.subjectType === '物理类' && filters.value.batch === '普通批本科';\n    };\n\n    return {\n      filters,\n      isValidSelection,\n      filteredData,\n      handleFilter,\n    };\n  },\n};\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>\n"],"mappings":"AA+BA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AACnC,OAAOC,SAAQ,MAAO,6BAA6B;AACnD,OAAOC,IAAG,MAAO,oBAAoB,EAAE;;AAEvC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,KAAKA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAIP,GAAG,CAAC;MAClBQ,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE;IACT,CAAC,CAAC;IAEF,MAAMC,gBAAe,GAAIV,GAAG,CAAC,KAAK,CAAC;IAEnC,MAAMW,YAAW,GAAIV,QAAQ,CAAC,MAAM;MAClC,IAAI,CAACS,gBAAgB,CAACE,KAAK,EAAE;QAC3B,OAAO,EAAE;MACX;MACA,OAAOT,IAAI,CAACU,MAAM,CAACC,IAAG,IACpBA,IAAI,CAAC,IAAI,MAAMP,OAAO,CAACK,KAAK,CAACJ,WAAU,IACvCM,IAAI,CAAC,IAAI,MAAMP,OAAO,CAACK,KAAK,CAACH,KAC/B,CAAC;IACH,CAAC,CAAC;IAEF,MAAMM,YAAW,GAAIA,CAAA,KAAM;MACzBL,gBAAgB,CAACE,KAAI,GAAIL,OAAO,CAACK,KAAK,CAACJ,WAAU,KAAM,KAAI,IAA