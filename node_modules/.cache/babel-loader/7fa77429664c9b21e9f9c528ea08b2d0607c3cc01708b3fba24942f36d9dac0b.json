{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  __name: 'AppHeader',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const name = computed({\n      get: () => store.state.studentInformation.name,\n      set: value => store.commit('setName', value)\n    });\n    const score = computed({\n      get: () => store.state.studentInformation.score,\n      set: value => store.commit('setScore', value)\n    });\n    const studentId = computed({\n      get: () => store.state.studentInformation.studentId,\n      set: value => store.commit('setStudentId', value)\n    });\n    const subject = computed({\n      get: () => store.state.studentInformation.subject,\n      set: value => store.commit('setSubject', value)\n    });\n    const setCandidate = () => {\n      alert('报考人信息输入成功');\n    };\n    const createArchive = () => {\n      const newArchive = {\n        name: store.state.studentInformation.name,\n        score: store.state.studentInformation.score,\n        studentId: store.state.studentInformation.studentId,\n        subject: store.state.studentInformation.subject,\n        data: []\n      };\n      const archiveName = `${newArchive.name} - ${newArchive.score} - ${newArchive.studentId} - ${newArchive.subject}`;\n      const archivesList = store.state.archivesList;\n      const tablenamelist = store.state.tablenamelist;\n      const isDuplicate = archivesList.some(archive => archive.name === newArchive.name && archive.score === newArchive.score && archive.studentId === newArchive.studentId && archive.subject === newArchive.subject);\n      if (isDuplicate) {\n        alert('不能创建重复的报考档案');\n      } else {\n        store.commit('addArchive', newArchive);\n        store.commit('addTableName', archiveName);\n        alert('报考档案创建成功');\n      }\n    };\n    const __returned__ = {\n      store,\n      name,\n      score,\n      studentId,\n      subject,\n      setCandidate,\n      createArchive,\n      computed,\n      get useStore() {\n        return useStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","useStore","store","name","get","state","studentInformation","set","value","commit","score","studentId","subject","setCandidate","alert","createArchive","newArchive","data","archiveName","archivesList","tablenamelist","isDuplicate","some","archive"],"sources":["/Users/jiangxingnandecomputer/Desktop/sss/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header :style=\"{ marginLeft: collapsed ? '80px' : '110px', transition: 'margin-left 0.2s', background: '#fff', padding: '0 24px', display: 'flex', alignItems: 'center', justifyContent: 'space-between', boxShadow: '0 2px 8px rgba(0, 0, 0, 0.1)' }\">\n    <div>\n      <a-row :gutter=\"16\" align=\"middle\">\n        <a-col>考生姓名</a-col>\n        <a-col>\n          <input v-model=\"name\" type=\"text\" style=\"width: 100px; height: 40px;\" >\n        </a-col>\n        <a-col>考生分数</a-col>\n        <a-col>\n          <input v-model=\"score\" type=\"text\" style=\"width: 100px; height: 40px;\" >\n        </a-col>\n        <a-col>考生考号</a-col>\n        <a-col>\n          <input v-model=\"studentId\" type=\"text\" style=\"width: 100px; height: 40px;\" >\n        </a-col>\n        <a-col>考生科目</a-col>\n        <a-col>\n          <input v-model=\"subject\" type=\"text\" style=\"width: 100px; height: 40px;\" >\n        </a-col>\n        <a-col>\n          当前报考人: {{ name }} - {{ score }} - {{ studentId }} - {{ subject }}\n        </a-col>\n      </a-row>\n    </div>\n    <div>\n      <a-space size=\"large\">\n        <a-button type=\"default\" @click=\"setCandidate\">设定报考人</a-button>\n        <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n      </a-space>\n    </div>\n  </a-layout-header>\n</template>\n\n<script setup>\nimport { computed } from 'vue'\nimport { useStore } from 'vuex'\n\nconst store = useStore()\n\nconst name = computed({\n  get: () => store.state.studentInformation.name,\n  set: value => store.commit('setName', value)\n})\nconst score = computed({\n  get: () => store.state.studentInformation.score,\n  set: value => store.commit('setScore', value)\n})\nconst studentId = computed({\n  get: () => store.state.studentInformation.studentId,\n  set: value => store.commit('setStudentId', value)\n})\nconst subject = computed({\n  get: () => store.state.studentInformation.subject,\n  set: value => store.commit('setSubject', value)\n})\n\nconst setCandidate = () => {\n  alert('报考人信息输入成功')\n}\n\nconst createArchive = () => {\n  const newArchive = {\n    name: store.state.studentInformation.name,\n    score: store.state.studentInformation.score,\n    studentId: store.state.studentInformation.studentId,\n    subject: store.state.studentInformation.subject,\n    data: []\n  }\n\n  const archiveName = `${newArchive.name} - ${newArchive.score} - ${newArchive.studentId} - ${newArchive.subject}`\n  const archivesList = store.state.archivesList\n  const tablenamelist = store.state.tablenamelist\n\n  const isDuplicate = archivesList.some(archive => \n    archive.name === newArchive.name && \n    archive.score === newArchive.score &&\n    archive.studentId === newArchive.studentId &&\n    archive.subject === newArchive.subject\n  )\n\n  if (isDuplicate) {\n    alert('不能创建重复的报考档案')\n  } else {\n    store.commit('addArchive', newArchive)\n    store.commit('addTableName', archiveName)\n    alert('报考档案创建成功')\n  }\n}\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>\n"],"mappings":"AAmCA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,SAASC,QAAQ,QAAQ,MAAM;;;;;;;IAE/B,MAAMC,KAAK,GAAGD,QAAQ,CAAC,CAAC;IAExB,MAAME,IAAI,GAAGH,QAAQ,CAAC;MACpBI,GAAG,EAAEA,CAAA,KAAMF,KAAK,CAACG,KAAK,CAACC,kBAAkB,CAACH,IAAI;MAC9CI,GAAG,EAAEC,KAAK,IAAIN,KAAK,CAACO,MAAM,CA