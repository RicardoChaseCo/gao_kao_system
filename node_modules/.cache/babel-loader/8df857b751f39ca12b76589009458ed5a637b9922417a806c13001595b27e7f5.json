{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { MenuFoldOutlined, MenuUnfoldOutlined } from '@ant-design/icons-vue';\nexport default {\n  setup(props, {\n    emit\n  }) {\n    const store = useStore();\n    const sidebarHidden = computed(() => props.sidebarHidden);\n    const name = computed({\n      get: () => store.state.studentInformation.name,\n      set: value => store.commit('setName', value)\n    });\n    const score = computed({\n      get: () => store.state.studentInformation.score,\n      set: value => store.commit('setScore', value)\n    });\n    const studentId = computed({\n      get: () => store.state.studentInformation.studentId,\n      set: value => store.commit('setStudentId', value)\n    });\n    const subject = computed({\n      get: () => store.state.studentInformation.subject,\n      set: value => store.commit('setSubject', value)\n    });\n    const toggleSidebar = () => {\n      emit('toggleSidebar');\n    };\n    const setCandidate = () => {\n      alert('报考人信息输入成功');\n    };\n    const createArchive = () => {\n      if (!name.value) {\n        alert('请先输入考生信息');\n        return;\n      }\n      store.commit('addTableName', name.value);\n      alert('报考档案创建成功');\n    };\n    return {\n      name,\n      score,\n      studentId,\n      subject,\n      toggleSidebar,\n      setCandidate,\n      createArchive\n    };\n  },\n  props: {\n    sidebarHidden: {\n      type: Boolean,\n      required: true\n    }\n  },\n  components: {\n    MenuFoldOutlined,\n    MenuUnfoldOutlined\n  }\n};","map":{"version":3,"names":["computed","useStore","MenuFoldOutlined","MenuUnfoldOutlined","setup","props","emit","store","sidebarHidden","name","get","state","studentInformation","set","value","commit","score","studentId","subject","toggleSidebar","setCandidate","alert","createArchive","type","Boolean","required","components"],"sources":["/Users/jiangxingnandecomputer/Desktop/ss/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header class=\"app-header\">\n    <div class=\"header-content\">\n      <a-row :gutter=\"16\" align=\"middle\" class=\"header-row\">\n        <img src=\"@/assets/logo.png\" alt=\"老姜高考\" style=\"width: 170px;\"/>\n        <a-col>\n          <div @click=\"toggleSidebar\" class=\"menu-icon\">\n            <MenuFoldOutlined v-if=\"!sidebarHidden\" :style=\"{ fontSize: '24px', color: '#007bff'}\" />\n            <MenuUnfoldOutlined v-else :style=\"{ fontSize: '24px', color: '#007bff' }\" />\n          </div>\n        </a-col>\n        <a-col>\n          考生姓名\n          <input v-model=\"name.value\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col class=\"col-hidden\">\n          考生分数\n          <input v-model=\"score.value\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col class=\"col-hidden\">\n          考生考号\n          <input v-model=\"studentId.value\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col class=\"col-hidden\">\n          考生位次\n          <input v-model=\"subject.value\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col class=\"styled-col compact-col\">\n          当前报考人: {{ name.value }} - {{ score.value }} - {{ studentId.value }} - {{ subject.value }}\n        </a-col>\n      </a-row>\n      <div>\n        <a-space size=\"large\" class=\"header-row\">\n          <a-button type=\"primary\" @click=\"setCandidate\">设定报考人</a-button>\n          <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n        </a-space>\n      </div>\n    </div>\n  </a-layout-header>\n</template>\n\n<script>\nimport { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { MenuFoldOutlined, MenuUnfoldOutlined } from '@ant-design/icons-vue';\n\nexport default {\n  setup(props, { emit }) {\n    const store = useStore();\n    \n    const sidebarHidden = computed(() => props.sidebarHidden);\n    \n    const name = computed({\n      get: () => store.state.studentInformation.name,\n      set: (value) => store.commit('setName', value),\n    });\n    \n    const score = computed({\n      get: () => store.state.studentInformation.score,\n      set: (value) => store.commit('setScore', value),\n    });\n    \n    const studentId = computed({\n      get: () => store.state.studentInformation.studentId,\n      set: (value) => store.commit('setStudentId', value),\n    });\n    \n    const subject = computed({\n      get: () => store.state.studentInformation.subject,\n      set: (value) => store.commit('setSubject', value),\n    });\n\n    const toggleSidebar = () => {\n      emit('toggleSidebar');\n    };\n\n    const setCandidate = () => {\n      alert('报考人信息输入成功');\n    };\n\n    const createArchive = () => {\n      if (!name.value) {\n        alert('请先输入考生信息');\n        return;\n      }\n      store.commit('addTableName', name.value);\n      alert('报考档案创建成功');\n    };\n\n    return {\n      name,\n      score,\n      studentId,\n      subject,\n      toggleSidebar,\n      setCandidate,\n      createArchive,\n    };\n  },\n  props: {\n    sidebarHidden: {\n      type: Boolean,\n      required: true,\n    },\n  },\n  components: {\n    MenuFoldOutlined,\n    MenuUnfoldOutlined,\n  },\n};\n</script>\n\n<style scoped>\n.menu-icon {\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n}\n\n.app-header {\n  border-bottom: 1px solid #ddd;\n  background-color: #f5f5f5; /* 确保背景色为浅色 */\n}\n\n.header-content {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  width: 100%;\n}\n\n.styled-input {\n  width: 100px;\n  height: 40px;\n  padding: 8px;\n  font-size: 0.8rem;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  transition: border-color 0.3s ease, box-shadow 0.3s ease;\n}\n\n.styled-input:hover {\n  border-color: #007bff;\n}\n\n.styled-input:focus {\n  border-color: #007bff;\n  box-shadow: 0 0 8px rgba(0, 123, 255, 0.5);\n  outline: none;\n}\n\n.styled-input::placeholder {\n  color: #aaa;\n  font-style: italic;\n}\n\n.styled-col {\n  border: 1px solid #007bff;\n  border-radius: 8px;\n  font-size: 0.8rem;\n  color: #333;\n  box-shadow: 0 4px 8px rgba(0, 123, 255, 0.2);\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n  margin-left: 20px;\n}\n\n.styled-col:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 6px 12px rgba(0, 123, 255, 0.3);\n}\n\n.styled-col span {\n  font-weight: bold;\n  color: #007bff;\n}\n\n.compact-col {\n  padding: 4px;\n  font-size: 0.8rem;\n  line-height: 2;\n}\n\n@media (max-width: 1527px) and (min-width: 768px) {\n  .compact-col {\n    display: none;\n  }\n}\n\n@media (max-width: 1311px) and (min-width: 768px) {\n  .col-hidden {\n    display: none;\n  }\n}\n\n@media (min-width: 768px) {\n  .header-logo {\n    display: none;\n  }\n}\n\n@media (max-width: 768px) {\n  .header-logo {\n    display: block;\n    height: 60px;\n  }\n\n}\n\n@media (max-width: 768px) {\n  .styled-input,\n  .styled-col,\n  .compact-col {\n    font-size: 0.8rem;\n  }\n  .header-row {\n    display: none;\n  }\n}\n</style>\n"],"mappings":"AA0CA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,gBAAgB,EAAEC,kBAAiB,QAAS,uBAAuB;AAE5E,eAAe;EACbC,KAAKA,CAACC,KAAK,EAAE;IAAEC;EAAK,CAAC,EAAE;IACrB,MAAMC,KAAI,GAAIN,QAAQ,CAAC,CAAC;IAExB,MAAMO,aAAY,GAAIR,QAAQ,CAAC,MAAMK,KAAK,CAACG,aAAa,CAAC;IAEzD,MAAMC,IAAG,GAAIT,QAAQ,CAAC;MACpBU,GAAG,EAAEA,CAAA,KAAMH,KAAK,CAACI,KAAK,CAACC,kBAAkB,CAACH,IAAI;MAC9CI,GAAG,EAAGC,KAAK,IAAKP,KAAK,CAACQ,MAAM,CAAC,SAAS,EAAED,KAAK;IAC/C,CAAC,CAAC;IAEF,MAAME,KAAI,GAAIhB,QAAQ,CAAC;MACrBU,GAAG,EAAEA,CAAA,KAAMH,KAAK,CAACI,KAAK,CAACC,kBAAkB,CAACI,KAAK;MAC/CH,GAAG,EAAGC,KAAK,IAAKP,KAAK,CAACQ,MAAM,CAAC,UAAU,EAAED,KAAK;IAChD,CAAC,CAAC;IAEF,MAAMG,SAAQ,GAAIjB,QAAQ,CAAC;MACzBU,GAAG,EAAEA,CAAA,KAAMH,KAAK,CAACI,KAAK,CAACC,kBAAkB,CAACK,SAAS;MACnDJ,GAAG,EAAGC,KAAK,IAAKP,KAAK,CAACQ,MAAM,CAAC,cAAc,EAAED,KAAK;IACpD,CAAC,CAAC;IAEF,MAAMI,OAAM,GAAIlB,QAAQ,CAAC;MACvBU,GAAG,EAAEA,CAAA,KAAMH,KAAK,CAACI,KAAK,C