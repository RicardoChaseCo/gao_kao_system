{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useStorage } from '@vueuse/core';\nimport { MenuFoldOutlined, MenuUnfoldOutlined } from '@ant-design/icons-vue';\n\n// Synchronize with local storage using useStorage\n\nexport default {\n  __name: 'AppHeader',\n  props: ['sidebarHidden'],\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const storedStudentInformation = useStorage('student-information', {\n      name: '',\n      score: 0,\n      studentId: '0',\n      subject: '0'\n    });\n\n    // Access the prop directly\n    const props = __props;\n    const store = useStore();\n\n    // Computed properties for state binding\n    const name = computed({\n      get: () => store.state.studentInformation.name,\n      set: value => {\n        store.commit('setName', value);\n        storedStudentInformation.value.name = value;\n      }\n    });\n    const score = computed({\n      get: () => store.state.studentInformation.score,\n      set: value => {\n        store.commit('setScore', value);\n        storedStudentInformation.value.score = value;\n      }\n    });\n    const studentId = computed({\n      get: () => store.state.studentInformation.studentId,\n      set: value => {\n        store.commit('setStudentId', value);\n        storedStudentInformation.value.studentId = value;\n      }\n    });\n    const subject = computed({\n      get: () => store.state.studentInformation.subject,\n      set: value => {\n        store.commit('setSubject', value);\n        storedStudentInformation.value.subject = value;\n      }\n    });\n\n    // Methods for component actions\n    const setCandidate = () => {\n      alert('报考人信息输入成功');\n    };\n    const createArchive = () => {\n      if (!name.value) {\n        alert('请先输入考生信息');\n        return;\n      }\n      store.commit('addTableName', name.value);\n      alert('报考档案创建成功');\n    };\n    const __returned__ = {\n      storedStudentInformation,\n      props,\n      store,\n      name,\n      score,\n      studentId,\n      subject,\n      setCandidate,\n      createArchive,\n      computed,\n      get useStore() {\n        return useStore;\n      },\n      get useStorage() {\n        return useStorage;\n      },\n      get MenuFoldOutlined() {\n        return MenuFoldOutlined;\n      },\n      get MenuUnfoldOutlined() {\n        return MenuUnfoldOutlined;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","useStore","useStorage","MenuFoldOutlined","MenuUnfoldOutlined","storedStudentInformation","name","score","studentId","subject","props","__props","store","get","state","studentInformation","set","value","commit","setCandidate","alert","createArchive"],"sources":["/Users/jiangxingnandecomputer/Desktop/ss/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header class=\"app-header\">\n    <div class=\"header-content\">\n      <a-row :gutter=\"16\" align=\"middle\" class=\"header-row\">\n        <img src=\"@/assets/logo.png\" alt=\"老姜高考\" style=\"width: 170px;\"/>\n        <a-col>\n          <div @click=\"$emit('toggleSidebar')\" class=\"menu-icon\">\n            <MenuFoldOutlined v-if=\"!sidebarHidden\" :style=\"{ fontSize: '24px', color: '#007bff'}\" />\n            <MenuUnfoldOutlined v-else :style=\"{ fontSize: '24px', color: '#007bff' }\" />\n          </div>\n        </a-col>\n        <a-col>\n          考生姓名\n          <input v-model=\"name\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col class=\"col-hidden\">\n          考生分数\n          <input v-model=\"score\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col class=\"col-hidden\">\n          考生考号\n          <input v-model=\"studentId\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col class=\"col-hidden\">\n          考生位次\n          <input v-model=\"subject\" type=\"text\" class=\"styled-input\">\n        </a-col>\n        <a-col class=\"styled-col compact-col\">\n          当前报考人: {{ name }} - {{ score }} - {{ studentId }} - {{ subject }}\n        </a-col>\n      </a-row>\n      <div>\n        <a-space size=\"large\" class=\"header-row\">\n          <a-button type=\"primary\" @click=\"setCandidate\">设定报考人</a-button>\n          <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n        </a-space>\n      </div>\n    </div>\n  </a-layout-header>\n</template>\n\n<script setup>\nimport { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useStorage } from '@vueuse/core';\nimport { MenuFoldOutlined, MenuUnfoldOutlined } from '@ant-design/icons-vue';\n\n// Synchronize with local storage using useStorage\nconst storedStudentInformation = useStorage('student-information', {\n  name: '',\n  score: 0,\n  studentId: '0',\n  subject: '0'\n});\n\n// Access the prop directly\nconst props = defineProps(['sidebarHidden']);\n\nconst store = useStore();\n\n// Computed properties for state binding\nconst name = computed({\n  get: () => store.state.studentInformation.name,\n  set: value => {\n    store.commit('setName', value);\n    storedStudentInformation.value.name = value;\n  }\n});\nconst score = computed({\n  get: () => store.state.studentInformation.score,\n  set: value => {\n    store.commit('setScore', value);\n    storedStudentInformation.value.score = value;\n  }\n});\nconst studentId = computed({\n  get: () => store.state.studentInformation.studentId,\n  set: value => {\n    store.commit('setStudentId', value);\n    storedStudentInformation.value.studentId = value;\n  }\n});\nconst subject = computed({\n  get: () => store.state.studentInformation.subject,\n  set: value => {\n    store.commit('setSubject', value);\n    storedStudentInformation.value.subject = value;\n  }\n});\n\n// Methods for component actions\nconst setCandidate = () => {\n  alert('报考人信息输入成功');\n};\n\nconst createArchive = () => {\n  if (!name.value) {\n    alert('请先输入考生信息');\n    return;\n  }\n\n  store.commit('addTableName', name.value);\n  alert('报考档案创建成功');\n};\n</script>\n\n\n<style scoped>\n.menu-icon {\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n}\n\n.app-header {\n  border-bottom: 1px solid #ddd;\n  background-color: #f5f5f5; /* 确保背景色为浅色 */\n}\n\n.header-content {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  width: 100%;\n}\n\n.styled-input {\n  width: 100px;\n  height: 40px;\n  padding: 8px;\n  font-size: 0.8rem;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  transition: border-color 0.3s ease, box-shadow 0.3s ease;\n}\n\n.styled-input:hover {\n  border-color: #007bff;\n}\n\n.styled-input:focus {\n  border-color: #007bff;\n  box-shadow: 0 0 8px rgba(0, 123, 255, 0.5);\n  outline: none;\n}\n\n.styled-input::placeholder {\n  color: #aaa;\n  font-style: italic;\n}\n\n.styled-col {\n  border: 1px solid #007bff;\n  border-radius: 8px;\n  font-size: 0.8rem;\n  color: #333;\n  box-shadow: 0 4px 8px rgba(0, 123, 255, 0.2);\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n  margin-left: 20px;\n}\n\n.styled-col:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 6px 12px rgba(0, 123, 255, 0.3);\n}\n\n.styled-col span {\n  font-weight: bold;\n  color: #007bff;\n}\n\n.compact-col {\n  padding: 4px;\n  font-size: 0.8rem;\n  line-height: 2;\n}\n\n@media (max-width: 1527px) and (min-width: 768px) {\n  .compact-col {\n    display: none;\n  }\n}\n\n@media (max-width: 1311px) and (min-width: 768px) {\n  .col-hidden {\n    display: none;\n  }\n}\n\n@media (min-width: 768px) {\n  .header-logo {\n    display: none;\n  }\n}\n\n@media (max-width: 768px) {\n  .header-logo {\n    display: block;\n    height: 60px;\n  }\n\n}\n\n@media (max-width: 768px) {\n  .styled-input,\n  .styled-col,\n  .compact-col {\n    font-size: 0.8rem;\n  }\n  .header-row {\n    display: none;\n 