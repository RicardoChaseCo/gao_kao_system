{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useToast } from 'vue3-toastify';\nexport default {\n  name: 'AppHeader',\n  props: {\n    collapsed: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      name: localStorage.getItem('currentName') || '',\n      score: localStorage.getItem('currentScore') || ''\n    };\n  },\n  computed: {\n    currentName() {\n      return localStorage.getItem('currentName');\n    },\n    currentScore() {\n      return localStorage.getItem('currentScore');\n    }\n  },\n  methods: {\n    setCandidate() {\n      localStorage.setItem('currentName', this.name);\n      localStorage.setItem('currentScore', this.score);\n    },\n    createArchive() {\n      const currentName = localStorage.getItem('currentName');\n      const currentScore = localStorage.getItem('currentScore');\n      const archiveName = `${currentName}${currentScore}`;\n      let archivesList = JSON.parse(localStorage.getItem('archivesList')) || [];\n      if (!archivesList.some(archive => archive.name === archiveName)) {\n        archivesList.push({\n          name: archiveName,\n          data: []\n        });\n        localStorage.setItem('archivesList', JSON.stringify(archivesList));\n        useToast().success(`${archiveName} 的报考档案构建成功`, {\n          position: \"top-right\",\n          autoClose: 3000\n        });\n      } else {\n        useToast().error(`${archiveName} 的报考档案已存在`, {\n          position: \"top-right\",\n          autoClose: 3000\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["useToast","name","props","collapsed","type","Boolean","default","data","localStorage","getItem","score","computed","currentName","currentScore","methods","setCandidate","setItem","createArchive","archiveName","archivesList","JSON","parse","some","archive","push","stringify","success","position","autoClose","error"],"sources":["/Users/jiangxingnandecomputer/Desktop/sss/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header :style=\"{ marginLeft: collapsed ? '80px' : '110px', transition: 'margin-left 0.2s', background: '#fff', padding: '0 24px', display: 'flex', alignItems: 'center', justifyContent: 'space-between', boxShadow: '0 2px 8px rgba(0, 0, 0, 0.1)' }\">\n    <div>\n      <a-row :gutter=\"16\" align=\"middle\">\n        <a-col>考生姓名</a-col>\n        <a-col>\n          <a-input v-model=\"name\" placeholder=\"姓名\" style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\" />\n        </a-col>\n        <a-col>考生分数</a-col>\n        <a-col>\n          <a-input v-model=\"score\" placeholder=\"分数\" style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\"/>\n        </a-col>\n      </a-row>\n    </div>\n    <div>\n      <a-space size=\"large\">\n        <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n        <a-button type=\"default\" @click=\"setCandidate\">设定报考人</a-button>\n      </a-space>\n    </div>\n    <div v-if=\"currentName && currentScore\">\n      当前报考人: {{ currentName }} - {{ currentScore }}\n    </div>\n  </a-layout-header>\n</template>\n\n<script>\nimport { useToast } from 'vue3-toastify';\n\nexport default {\n  name: 'AppHeader',\n  props: {\n    collapsed: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  data() {\n    return {\n      name: localStorage.getItem('currentName') || '',\n      score: localStorage.getItem('currentScore') || '',\n    };\n  },\n  computed: {\n    currentName() {\n      return localStorage.getItem('currentName');\n    },\n    currentScore() {\n      return localStorage.getItem('currentScore');\n    },\n  },\n  methods: {\n    setCandidate() {\n      localStorage.setItem('currentName', this.name);\n      localStorage.setItem('currentScore', this.score);\n    },\n    createArchive() {\n      const currentName = localStorage.getItem('currentName');\n      const currentScore = localStorage.getItem('currentScore');\n      const archiveName = `${currentName}${currentScore}`;\n      let archivesList = JSON.parse(localStorage.getItem('archivesList')) || [];\n\n      if (!archivesList.some(archive => archive.name === archiveName)) {\n        archivesList.push({ name: archiveName, data: [] });\n        localStorage.setItem('archivesList', JSON.stringify(archivesList));\n        useToast().success(`${archiveName} 的报考档案构建成功`, {\n          position: \"top-right\",\n          autoClose: 3000,\n        });\n      } else {\n        useToast().error(`${archiveName} 的报考档案已存在`, {\n          position: \"top-right\",\n          autoClose: 3000,\n        });\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>\n"],"mappings":";AA2BA,SAASA,QAAO,QAAS,eAAe;AAExC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLN,IAAI,EAAEO,YAAY,CAACC,OAAO,CAAC,aAAa,KAAK,EAAE;MAC/CC,KAAK,EAAEF,YAAY,CAACC,OAAO,CAAC,cAAc,KAAK;IACjD,CAAC;EACH,CAAC;EACDE,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAOJ,YAAY,CAACC,OAAO,CAAC,aAAa,CAAC;IAC5C,CAAC;IACDI,YAAYA,CAAA,EAAG;MACb,OAAOL,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC;IAC7C;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACbP,YAAY,CAACQ,OAAO,CAAC,aAAa,EAAE,IAAI,CAACf,IAAI,CAAC;MAC9CO,YAAY,CAACQ,OAAO,CAAC,cAAc,EAAE,IAAI,CAACN,KAAK,CAAC;IAClD,CAAC;IACDO,aAAaA,CAAA,EAAG;MACd,MAAML,WAAU,GAAIJ,YAAY,CAACC,OAAO,CAAC,aAAa,CAAC;MACvD,MAAMI,YAAW,GAAIL,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC;MACzD,MAAMS,WAAU,GAAI,GAAGN,WAAW,GAAGC,YAAY,EAAE;MACnD,IAAIM,YAAW,GAAIC,IAAI,CAACC,KAAK,CAACb,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC,KAAK,EAAE;MAEzE,IAAI,CAACU,YAAY,CAACG,IAAI,CAACC,OAAM,IAAKA,OAAO,CAACtB,IAAG,KAAMiB,WAAW,CAAC,EAAE;QAC/DC,YAAY,CAACK,IAAI,CAAC;UAAEvB,IAAI,EAAEiB,WAAW;UAAEX,IAAI,EAAE;QAAG,CAAC,CAAC;QAClDC,YAAY,CAACQ,OAAO,CAAC,cAAc,EAAEI,IAAI,CAACK,SAAS,CAACN,YAAY,CAAC,CAAC;QAClEnB,QAAQ,CAAC,CAAC,CAAC0B,OAAO,CAAC,GAAGR,WAAW,YAAY,EAAE;UAC7CS,QAAQ,EAAE,WAAW;UACrBC,SAAS,EAAE;QACb,CAAC,CAAC;MACJ,OA