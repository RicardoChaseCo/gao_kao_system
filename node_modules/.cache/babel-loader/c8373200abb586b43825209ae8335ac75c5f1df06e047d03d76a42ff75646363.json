urrentName,\n      currentScore: this.currentScore\n    };\n  },\n  methods: {\n    setCandidate() {\n      this.currentName = this.name;\n      this.currentScore = this.score;\n      localStorage.setItem('currentName', this.name);\n      localStorage.setItem('currentScore', this.score);\n    },\n    createArchive() {\n      const currentName = this.currentName;\n      const currentScore = this.currentScore;\n      const archiveName = `${currentName}${currentScore}`;\n      let archivesList = JSON.parse(localStorage.getItem('archivesList')) || [];\n\n      if (!archivesList.some(archive => archive.name === archiveName)) {\n        archivesList.push({ name: archiveName, data: [] });\n        localStorage.setItem('archivesList', JSON.stringify(archivesList));\n        alert(`${archiveName} 的报考档案构建成功`);\n      } else {\n        alert(`${archiveName} 的报考档案已存在`);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>\n"],"mappings":";AA2BA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLN,IAAI,EAAE,EAAE;MACRO,KAAK,EAAE,EAAE;MACTC,WAAW,EAAEC,YAAY,CAACC,OAAO,CAAC,aAAa,KAAK,EAAE;MACtDC,YAAY,EAAEF,YAAY,CAACC,OAAO,CAAC,cAAc,KAAK;IACxD,CAAC;EACH,CAAC;EACDE,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,OAAO,GAAG,IAAI,CAACL,WAAW,MAAM,IAAI,CAACG,YAAY,EAAE;IACrD;EACF,CAAC;EACDG,KAAK,EAAE;IACLd,IAAIA,CAACe,GAAG,EAAE;MACRN,YAAY,CAACO,OAAO,CAAC,aAAa,EAAED,GAAG,CAAC;MACxC,IAAI,CAACP,WAAU,GAAIO,GAAG;IACxB,CAAC;IACDR,KAAKA,CAACQ,GAAG,EAAE;MACTN,YAAY,CAACO,OAAO,CAAC,cAAc,EAAED,GAAG,CAAC;MACzC,IAAI,CAACJ,YAAW,GAAII,GAAG;IACzB;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,OAAO;MACLT,WAAW,EAAE,IAAI,CAACA,WAAW;MAC7BG,YAAY,EAAE,IAAI,CAACA;IACrB,CAAC;EACH,CAAC;EACDO,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACX,WAAU,GAAI,IAAI,CAACR,IAAI;MAC5B,IAAI,CAACW,YAAW,GAAI,IAAI,CAACJ,KAAK;MAC9BE,YAAY,CAACO,OAAO,CAAC,aAAa,EAAE,IAAI,CAAChB,IAAI,CAAC;MAC9CS,YAAY,CAACO,OAAO,CAAC,cAAc