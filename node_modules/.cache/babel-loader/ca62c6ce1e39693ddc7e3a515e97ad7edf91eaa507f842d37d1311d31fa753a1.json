{"ast":null,"code":"import { mapState, mapMutations } from 'vuex';\nexport default {\n  data() {\n    return {\n      headerWidth: 'calc(100% - 210px)',\n      buttonContainerMarginRight: '0px',\n      showDropdown: false\n    };\n  },\n  computed: {\n    ...mapState({\n      name: state => state.studentInformation.name,\n      score: state => state.studentInformation.score,\n      studentId: state => state.studentInformation.studentId,\n      subject: state => state.studentInformation.subject\n    })\n  },\n  methods: {\n    ...mapMutations({\n      setName: 'setName',\n      setScore: 'setScore',\n      setStudentId: 'setStudentId',\n      setSubject: 'setSubject',\n      addTableName: 'addTableName'\n    }),\n    setCandidate() {\n      alert('报考人信息输入成功');\n    },\n    createArchive() {\n      if (!this.name) {\n        alert('请先输入考生信息');\n        return;\n      }\n      this.addTableName(this.name);\n      alert('报考档案创建成功');\n    },\n    updateHeaderWidth() {\n      if (window.innerWidth <= 1527 && window.innerWidth >= 1311) {\n        this.showDropdown = false;\n      } else if (window.innerWidth < 1311 && window.innerWidth >= 768) {\n        this.showDropdown = true;\n      } else {\n        this.showDropdown = false;\n      }\n    }\n  },\n  mounted() {\n    this.updateHeaderWidth();\n    window.addEventListener('resize', this.updateHeaderWidth);\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.updateHeaderWidth);\n  }\n};","map":{"version":3,"names":["mapState","mapMutations","data","headerWidth","buttonContainerMarginRight","showDropdown","computed","name","state","studentInformation","score","studentId","subject","methods","setName","setScore","setStudentId","setSubject","addTableName","setCandidate","alert","createArchive","updateHeaderWidth","window","innerWidth","mounted","addEventListener","beforeUnmount","removeEventListener"],"sources":["/Users/jiangxingnan/Desktop/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header class=\"app-header\">\n    <div class=\"header-content\">\n      <div>\n        <a-row :gutter=\"16\" align=\"middle\" class=\"header-row\">\n          <a-col>考生姓名</a-col>\n          <a-col>\n            <input v-model=\"name\" type=\"text\" class=\"styled-input\">\n          </a-col>\n          <a-dropdown v-if=\"showDropdown\" trigger=\"click\">\n            <template #default>\n              <a-button icon=\"ellipsis\" />\n            </template>\n            <template #overlay>\n              <a-menu>\n                <a-menu-item key=\"score\">\n                  <span>考生分数</span>\n                  <input v-model=\"score\" type=\"text\" class=\"styled-input dropdown-input\" />\n                </a-menu-item>\n                <a-menu-item key=\"studentId\">\n                  <span>考生考号</span>\n                  <input v-model=\"studentId\" type=\"text\" class=\"styled-input dropdown-input\" />\n                </a-menu-item>\n                <a-menu-item key=\"subject\">\n                  <span>考生位次</span>\n                  <input v-model=\"subject\" type=\"text\" class=\"styled-input dropdown-input\" />\n                </a-menu-item>\n              </a-menu>\n            </template>\n          </a-dropdown>\n          <a-col v-else>考生分数</a-col>\n          <a-col v-else>\n            <input v-model=\"score\" type=\"text\" class=\"styled-input\">\n          </a-col>\n          <a-col v-else>考生考号</a-col>\n          <a-col v-else>\n            <input v-model=\"studentId\" type=\"text\" class=\"styled-input\">\n          </a-col>\n          <a-col v-else>考生位次</a-col>\n          <a-col v-else>\n            <input v-model=\"subject\" type=\"text\" class=\"styled-input\">\n          </a-col>\n          <a-col class=\"styled-col compact-col\">\n            当前报考人: {{ name }} - {{ score }} - {{ studentId }} - {{ subject }}\n          </a-col>\n        </a-row>\n      </div>\n      <div>\n        <a-space size=\"large\" class=\"header-row\">\n          <a-button type=\"primary\" @click=\"setCandidate\">设定报考人</a-button>\n          <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n        </a-space>\n      </div>\n    </div>\n  </a-layout-header>\n</template>\n\n<script>\nimport { mapState, mapMutations } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      headerWidth: 'calc(100% - 210px)',\n      buttonContainerMarginRight: '0px',\n      showDropdown: false,\n    };\n  },\n  computed: {\n    ...mapState({\n      name: state => state.studentInformation.name,\n      score: state => state.studentInformation.score,\n      studentId: state => state.studentInformation.studentId,\n      subject: state => state.studentInformation.subject,\n    })\n  },\n  methods: {\n    ...mapMutations({\n      setName: 'setName',\n      setScore: 'setScore',\n      setStudentId: 'setStudentId',\n      setSubject: 'setSubject',\n      addTableName: 'addTableName',\n    }),\n    setCandidate() {\n      alert('报考人信息输入成功');\n    },\n    createArchive() {\n      if (!this.name) {\n        alert('请先输入考生信息');\n        return;\n      }\n      this.addTableName(this.name);\n      alert('报考档案创建成功');\n    },\n    updateHeaderWidth() {\n      if (window.innerWidth <= 1527 && window.innerWidth >= 1311) {\n        this.showDropdown = false;\n      } else if (window.innerWidth < 1311 && window.innerWidth >= 768) {\n        this.showDropdown = true;\n      } else {\n        this.showDropdown = false;\n      }\n    }\n  },\n  mounted() {\n    this.updateHeaderWidth();\n    window.addEventListener('resize', this.updateHeaderWidth);\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.updateHeaderWidth);\n  }\n};\n</script>\n\n<style scoped>\n.app-header {\n  border-bottom: 1px solid #ddd;\n  background-color: #f5f5f5; /* Ensure light background color */\n  transition: margin-left 0.2s, width 0.2s;\n}\n\n.header-content {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  width: 100%;\n}\n\n.styled-input {\n  width: 100px;\n  height: 40px;\n  padding: 8px;\n  font-size: 0.8rem;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  transition: border-color 0.3s ease, box-shadow 0.3s ease;\n}\n\n.styled-input:hover {\n  border-color: #007bff;\n}\n\n.styled-input:focus {\n  border-color: #007bff;\n  box-shadow: 0 0 8px rgba(0, 123, 255, 0.5);\n  outline: none;\n}\n\n.styled-input::placeholder {\n  color: #aaa;\n  font-style: italic;\n}\n\n.styled-col {\n  border: 1px solid #007bff;\n  border-radius: 8px;\n  font-size: 0.8rem;\n  color: #333;\n  box-shadow: 0 4px 8px rgba(0, 123, 255, 0.2);\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n  margin-left: 20px;\n}\n\n.styled-col:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 6px 12px rgba(0, 123, 255, 0.3);\n}\n\n.styled-col span {\n  font-weight: bold;\n  color: #007bff;\n}\n\n.compact-col {\n  padding: 4px;\n  font-size: 0.8rem;\n  line-height: 2;\n}\n\n.header-row {\n  min-width: 1500px; /* Adjust based on your content width */\n}\n\n.button-container {\n  display: flex;\n  justify-content: flex-end;\n  flex-grow: 1;\n  margin-right: 0; /* Default to 0, adjusted dynamically */\n}\n\n/* Media queries for different screen sizes */\n@media (max-width: 1527px) and (min-width: 1311px) {\n  .compact-col {\n    display: none;\n  }\n}\n\n@media (max-width: 1311px) and (min-width: 768px) {\n  .dropdown-input {\n    width: 100%;\n  }\n  .header-row > .compact-col {\n    display: none;\n  }\n  .header-row > a-col {\n    display: none;\n  }\n}\n\n@media (max-width: 768px) {\n  .header-content {\n    display: none;\n  }\n}\n</style>\n"],"mappings":"AA0DA,SAASA,QAAQ,EAAEC,YAAW,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,oBAAoB;MACjCC,0BAA0B,EAAE,KAAK;MACjCC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGN,QAAQ,CAAC;MACVO,IAAI,EAAEC,KAAI,IAAKA,KAAK,CAACC,kBAAkB,CAACF,IAAI;MAC5CG,KAAK,EAAEF,KAAI,IAAKA,KAAK,CAACC,kBAAkB,CAACC,KAAK;