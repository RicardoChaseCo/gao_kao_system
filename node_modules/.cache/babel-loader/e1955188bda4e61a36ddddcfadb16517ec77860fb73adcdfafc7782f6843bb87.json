{"ast":null,"code":"import { mapState, mapMutations } from 'vuex';\nexport default {\n  name: 'AppHeader',\n  props: {\n    collapsed: {\n      type: Boolean,\n      default: false\n    }\n  },\n  computed: {\n    ...mapState(['name', 'score']),\n    currentCandidate() {\n      return `${this.name} - ${this.score}`;\n    }\n  },\n  methods: {\n    ...mapMutations(['setName', 'setScore', 'addArchive']),\n    setCandidate() {\n      this.setName(this.name);\n      this.setScore(this.score);\n    },\n    createArchive() {\n      const archiveName = `${this.name}${this.score}`;\n      let archivesList = this.$store.state.archivesList;\n      if (!archivesList.some(archive => archive.name === archiveName)) {\n        this.addArchive({\n          name: archiveName,\n          data: []\n        });\n        alert(`${archiveName} 的报考档案构建成功`);\n      } else {\n        alert(`${archiveName} 的报考档案已存在`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapMutations","name","props","collapsed","type","Boolean","default","computed","currentCandidate","score","methods","setCandidate","setName","setScore","createArchive","archiveName","archivesList","$store","state","some","archive","addArchive","data","alert"],"sources":["/Users/jiangxingnandecomputer/Desktop/sss/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header :style=\"{ marginLeft: collapsed ? '80px' : '110px', transition: 'margin-left 0.2s', background: '#fff', padding: '0 24px', display: 'flex', alignItems: 'center', justifyContent: 'space-between', boxShadow: '0 2px 8px rgba(0, 0, 0, 0.1)' }\">\n    <div>\n      <a-row :gutter=\"16\" align=\"middle\">\n        <a-col>考生姓名</a-col>\n        <a-col>\n          <a-input v-model=\"name\" placeholder=\"姓名\" style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\" />\n        </a-col>\n        <a-col>考生分数</a-col>\n        <a-col>\n          <a-input v-model=\"score\" placeholder=\"分数\" style=\"width: 200px; padding-top: 10px; padding-bottom: 8px;\" />\n        </a-col>\n        <a-col>\n          当前报考人: {{ currentCandidate }}\n        </a-col>\n      </a-row>\n    </div>\n    <div>\n      <a-space size=\"large\">\n        <a-button type=\"default\" @click=\"setCandidate\">设定报考人</a-button>\n        <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n      </a-space>\n    </div>\n  </a-layout-header>\n</template>\n\n<script>\nimport { mapState, mapMutations } from 'vuex';\n\nexport default {\n  name: 'AppHeader',\n  props: {\n    collapsed: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  computed: {\n    ...mapState(['name', 'score']),\n    currentCandidate() {\n      return `${this.name} - ${this.score}`;\n    }\n  },\n  methods: {\n    ...mapMutations(['setName', 'setScore', 'addArchive']),\n    setCandidate() {\n      this.setName(this.name);\n      this.setScore(this.score);\n    },\n    createArchive() {\n      const archiveName = `${this.name}${this.score}`;\n      let archivesList = this.$store.state.archivesList;\n\n      if (!archivesList.some(archive => archive.name === archiveName)) {\n        this.addArchive({ name: archiveName, data: [] });\n        alert(`${archiveName} 的报考档案构建成功`);\n      } else {\n        alert(`${archiveName} 的报考档案已存在`);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>\n"],"mappings":"AA2BA,SAASA,QAAQ,EAAEC,YAAW,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGR,QAAQ,CAAC,CAAC,MAAM,EAAE,OAAO,CAAC,CAAC;IAC9BS,gBAAgBA,CAAA,EAAG;MACjB,OAAO,GAAG,IAAI,CAACP,IAAI,MAAM,IAAI,CAACQ,KAAK,EAAE;IACvC;EACF,CAAC;EACDC,OAAO,EAAE;IACP,GAAGV,YAAY,CAAC,CAAC,SAAS,EAAE,UAAU,EAAE,YAAY,CAAC,CAAC;IACtDW,YAAYA,CAAA,EAAG;MACb,IAAI,CAACC,OAAO,CAAC,IAAI,CAACX,IAAI,CAAC;MACvB,IAAI,CAACY,QAAQ,CAAC,IAAI,CAACJ,KAAK,CAAC;IAC3B,CAAC;IACDK,aAAaA,CAAA,EAAG;MACd,MAAMC,WAAU,GAAI,GAAG,IAAI,CAACd,IAA