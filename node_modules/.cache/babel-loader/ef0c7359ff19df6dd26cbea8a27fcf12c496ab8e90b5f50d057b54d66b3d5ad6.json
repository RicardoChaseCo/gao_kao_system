{"ast":null,"code":"import { mapState, mapMutations } from 'vuex';\nexport default {\n  data() {\n    return {\n      headerWidth: 'calc(100% - 210px)',\n      buttonContainerMarginRight: '0px'\n    };\n  },\n  computed: {\n    ...mapState({\n      name: state => state.studentInformation.name,\n      score: state => state.studentInformation.score,\n      studentId: state => state.studentInformation.studentId,\n      subject: state => state.studentInformation.subject\n    })\n  },\n  methods: {\n    ...mapMutations({\n      setName: 'setName',\n      setScore: 'setScore',\n      setStudentId: 'setStudentId',\n      setSubject: 'setSubject',\n      addTableName: 'addTableName'\n    }),\n    setCandidate() {\n      alert('报考人信息输入成功');\n    },\n    createArchive() {\n      if (!this.name) {\n        alert('请先输入考生信息');\n        return;\n      }\n      this.addTableName(this.name);\n      alert('报考档案创建成功');\n    },\n    updateHeaderWidth() {\n      this.headerWidth = `${window.innerWidth - 210}px`;\n      if (window.innerWidth <= 2033 && window.innerWidth >= 1440) {\n        this.buttonContainerMarginRight = '210px';\n      } else {\n        this.buttonContainerMarginRight = '0px';\n      }\n    }\n  },\n  mounted() {\n    this.updateHeaderWidth();\n    window.addEventListener('resize', this.updateHeaderWidth);\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.updateHeaderWidth);\n  }\n};","map":{"version":3,"names":["mapState","mapMutations","data","headerWidth","buttonContainerMarginRight","computed","name","state","studentInformation","score","studentId","subject","methods","setName","setScore","setStudentId","setSubject","addTableName","setCandidate","alert","createArchive","updateHeaderWidth","window","innerWidth","mounted","addEventListener","beforeUnmount","removeEventListener"],"sources":["/Users/jiangxingnan/Desktop/gao_kao_system/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <a-layout-header :style=\"{ width: headerWidth }\" class=\"app-header\">\n    <div class=\"header-content\">\n      <div class=\"responsive-container\">\n        <a-row :gutter=\"16\" align=\"middle\" class=\"header-row\">\n          <a-col>考生姓名</a-col>\n          <a-col>\n            <input v-model=\"name\" type=\"text\" class=\"styled-input\">\n          </a-col>\n          <a-col>考生分数</a-col>\n          <a-col>\n            <input v-model=\"score\" type=\"text\" class=\"styled-input\">\n          </a-col>\n          <a-col>考生考号</a-col>\n          <a-col>\n            <input v-model=\"studentId\" type=\"text\" class=\"styled-input\">\n          </a-col>\n          <a-col>考生位次</a-col>\n          <a-col>\n            <input v-model=\"subject\" type=\"text\" class=\"styled-input\">\n          </a-col>\n          <a-col class=\"styled-col compact-col\">\n            当前报考人: {{ name }} - {{ score }} - {{ studentId }} - {{ subject }}\n          </a-col>\n        </a-row>\n      </div>\n      <div class=\"button-container\">\n        <a-space size=\"large\" class=\"header-buttons\">\n          <a-button type=\"primary\" @click=\"setCandidate\">设定报考人</a-button>\n          <a-button type=\"primary\" @click=\"createArchive\">创建报考档案</a-button>\n        </a-space>\n      </div>\n    </div>\n  </a-layout-header>\n</template>\n\n<script>\nimport { mapState, mapMutations } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      headerWidth: 'calc(100% - 210px)',\n      buttonContainerMarginRight: '0px'\n    };\n  },\n  computed: {\n    ...mapState({\n      name: state => state.studentInformation.name,\n      score: state => state.studentInformation.score,\n      studentId: state => state.studentInformation.studentId,\n      subject: state => state.studentInformation.subject,\n    })\n  },\n  methods: {\n    ...mapMutations({\n      setName: 'setName',\n      setScore: 'setScore',\n      setStudentId: 'setStudentId',\n      setSubject: 'setSubject',\n      addTableName: 'addTableName',\n    }),\n    setCandidate() {\n      alert('报考人信息输入成功');\n    },\n    createArchive() {\n      if (!this.name) {\n        alert('请先输入考生信息');\n        return;\n      }\n      this.addTableName(this.name);\n      alert('报考档案创建成功');\n    },\n    updateHeaderWidth() {\n      this.headerWidth = `${window.innerWidth - 210}px`;\n      if (window.innerWidth <= 2033 && window.innerWidth >= 1440) {\n        this.buttonContainerMarginRight = '210px';\n      } else {\n        this.buttonContainerMarginRight = '0px';\n      }\n    }\n  },\n  mounted() {\n    this.updateHeaderWidth();\n    window.addEventListener('resize', this.updateHeaderWidth);\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.updateHeaderWidth);\n  }\n};\n</script>\n\n<style scoped>\n.app-header {\n  border-bottom: 1px solid #ddd;\n  background-color: #f5f5f5; /* Ensure light background color */\n}\n\n.header-content {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  width: 100%;\n}\n\n.styled-input {\n  width: 100px;\n  height: 40px;\n  padding: 8px;\n  font-size: 0.8rem;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  transition: border-color 0.3s ease, box-shadow 0.3s ease;\n}\n\n.styled-input:hover {\n  border-color: #007bff;\n}\n\n.styled-input:focus {\n  border-color: #007bff;\n  box-shadow: 0 0 8px rgba(0, 123, 255, 0.5);\n  outline: none;\n}\n\n.styled-input::placeholder {\n  color: #aaa;\n  font-style: italic;\n}\n\n.styled-col {\n  border: 1px solid #007bff;\n  border-radius: 8px;\n  font-size: 0.8rem;\n  color: #333;\n  box-shadow: 0 4px 8px rgba(0, 123, 255, 0.2);\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n  margin-left: 20px;\n}\n\n.styled-col:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 6px 12px rgba(0, 123, 255, 0.3);\n}\n\n.styled-col span {\n  font-weight: bold;\n  color: #007bff;\n}\n\n.compact-col {\n  padding: 4px;\n  font-size: 0.8rem;\n  line-height: 2;\n}\n\n.header-row {\n  min-width: 1500px; /* Adjust based on your content width */\n}\n\n.button-container {\n  display: flex;\n  justify-content: flex-end;\n  flex-grow: 1;\n  margin-right: 0; /* Default to 0, adjusted dynamically */\n}\n\n/* Media queries for different screen sizes */\n\n/* Screen width 2560px and above: app-header displayed normally */\n@media (min-width: 2560px) {\n  .responsive-container {\n    width: 100%;\n    overflow-x: visible;\n  }\n}\n\n/* Screen width between 1440px and 2560px: Elements in overflow-x container */\n@media (max-width: 2560px) and (min-width: 1440px) {\n  .responsive-container {\n    width: 100%;\n    overflow-x: visible;\n  }\n}\n\n/* Screen width between 1440px and 768px: Elements in overflow-x container */\n@media (max-width: 1440px) and (min-width: 768px) {\n  .responsive-container {\n    display: flex;\n    width: 100%;\n    overflow-x: auto;\n  }\n\n  .header-row {\n    display: flex;\n    white-space: nowrap;\n  }\n\n  .button-container {\n    flex-shrink: 0;\n    white-space: nowrap;\n  }\n}\n\n/* Screen width below 768px: Hide all content */\n@media (max-width: 768px) {\n  .header-content {\n    display: none;\n  }\n}\n</style>\n"],"mappings":"AAqCA,SAASA,QAAQ,EAAEC,YAAW,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,oBAAoB;MACjCC,0BAA0B,EAAE;IAC9B,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGL,QAAQ,CAAC;MACVM,IAAI,EAAEC,KAAI,IAAKA,KAAK,CAACC,kBAAkB,CAACF,IAAI;MAC5CG,KAAK,EAAEF,KAAI,IAAKA,KAAK,CAACC,kBAAkB,CAACC,KAAK;MAC9CC,SAAS,EAAEH,KAAI,IAAKA,KAAK,CAACC,kBAAkB,CAACE,SAAS;MACtDC,OAAO,EAAEJ,KAAI,IAAKA,KAAK,CAACC,kBAAkB,CAACG;IAC7C,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,GAAGX,YAAY,CAAC;MACdY,OAAO,EAAE,SAAS;MAClBC,QAAQ,EAAE,UAAU;MACpBC,YAAY,EAAE,cAAc;MAC5BC,UAAU,EAAE,YAAY;MACxBC,YAAY,EAAE;IAChB,CAAC,CAAC;IACFC,YAAYA,CAAA,EAAG;MACbC,KAAK,CAAC,WAAW,CAAC;IACpB,CAAC;IACDC,aAAaA,CAAA,EAAG;MACd,IAAI,CAAC,IAAI,CAACd,IAAI,EAAE;QACda,KAAK,CAAC,UAAU,CAAC;QACjB;MACF;MACA,IAAI,CAACF,YAAY,CAAC,IAAI,CAACX,IAAI,CAAC;MAC5Ba,KAAK,CAAC,UAAU,CAAC;IACnB,CAAC;IACDE,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAA